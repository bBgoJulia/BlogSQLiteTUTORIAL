<table>
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nome</th>
      <th scope="col">Email</th>
      <th scope="col">Celular</th>
    </tr>
  </thead>
  <tbody>
    <% for (let index = 0; index < 9; index++) { %>
    <tr>

      <% if (index % 2 == 0) { %>

      <td><%= index %></td>
      <td>NOME</td>
      <td>EMAIL</td>
      <td>CELULAR</td>
    </tr>
    <% } %>  <%} %>
  </tbody>
</table>
